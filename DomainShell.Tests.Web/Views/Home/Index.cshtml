@{
    ViewBag.Title = "Home Page";
}

@section scripts{            
    <script src="~/scripts/require.js"></script> 
    <script type="text/javascript">
        require.config({
            urlArgs: "bust=v2",
            baseUrl: "/scripts",
        });

        require(["figuram", "home/listApp"], function (figuram, app) {
            var el = document.getElementById("sample");            
            figuram.render(el, app);
        });
</script>
}

<h2>Index</h2>

<div class="row">
    <div class="col-md-12">                                
        <div id="sample"></div>        
    </div>    
</div>